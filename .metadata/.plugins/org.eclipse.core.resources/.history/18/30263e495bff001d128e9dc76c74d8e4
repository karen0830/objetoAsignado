
public class Perros {
	String color;
	int años;
	String raza;
	double peso;
	
	public Perros(){
		
	}
	
	public Perros(String color, int años, String raza, double peso) {
		this.color = color;
		this.años = años;
		this.raza = raza;
		this.peso = peso;
	}
	//encapsulación
	//color
	public void setColor(String color) {
		this.color = color;
	}
	
	public String getColor() {
		return color;
	}
	
	//años
	public void setAños(int años) {
		this.años = años;
	}
	
	public int getAños() {
		return años;
	}
	//
}
